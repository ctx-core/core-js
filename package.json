{
	"name": "@ctx-core/core-js",
	"version": "10.0.19",
	"description": "ctx-core core-js",
	"keywords": [
		"ctx-core",
		"core-js"
	],
	"homepage": "https://github.com/ctx-core/core-js#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/core-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/core-js/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^20.4.2",
		"@ctx-core/package": "workspace:^8.0.16"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}