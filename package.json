{
	"name": "@ctx-core/core-js",
	"version": "11.2.178",
	"description": "ctx-core core-js",
	"keywords": [
		"ctx-core",
		"core-js"
	],
	"homepage": "https://github.com/ctx-core/core-js#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/core-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/core-js.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./CORE_JS_SHIM_URL",
		"./CORE_JS_SHIM_URL_",
		"./core_js_version_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/package": "^9.3.142"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}